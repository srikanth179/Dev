{
	"name": "archivesink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"schema_name": {
				"type": "string"
			},
			"current_date": {
				"type": "string"
			},
			"Archive_folder": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@concat(dataset().schema_name,'/',dataset().Archive_folder,'/',dataset().current_date)",
					"type": "Expression"
				},
				"fileSystem": "rawzone"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}