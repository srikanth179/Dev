{
	"name": "archivesink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"schema_name": {
				"type": "string"
			},
			"archive_folder": {
				"type": "string"
			},
			"current_date": {
				"type": "string"
			},
			"table_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@concat(dataset().schema_name,'/',dataset().archive_folder,'/',dataset().current_date,'/',dataset().table_name)",
					"type": "Expression"
				},
				"fileSystem": "rawzone"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": ""
		},
		"schema": []
	}
}